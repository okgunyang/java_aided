package builder;

public class Jacket {
	int number;
	String name;
	double size;
	
	private Jacket(int number, String name, double size){
		this.number = number;
		this.name = name;
		this.size = size;
	}
	
	public static class Builder {
		int number = 0;
		String name = null;
		double size = 0.0;
		public Builder() { }
		
	}
}	